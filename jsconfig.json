{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ES6",
        "checkJs": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "alwaysStrict": true,
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}